{
    "name": "youthage/laravel-geohash",
    "description": "LBS地理位置距离计算方法geohash，将一个经纬度信息，转换成一个可以排序，可以比较的字符串编码，用于高效搜索",
    "require": {
        "php": ">=5.6"
    },
    "license": "MIT",
    "authors": [
        {
            "name": "YouthAge",
            "email": "myouthage@gmail.com"
        }
    ],
    "minimum-stability": "dev",
    "autoload": {
        "psr-4": {
            "Geohash\\": "src/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "Geohash\\GeoHashProvider"
            ],
            "aliases": {
                "GeoHash": "Geohash\\GeoHashFacade"
            }
        }
    }
}
